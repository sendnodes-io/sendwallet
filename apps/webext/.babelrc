{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": ["chrome >= 100", "firefox >= 100", "safari >= 16"]
        }
      }
    ],
    // Because babel is used by Webpack to load the Webpack config, which is
    // TS.
    "@babel/typescript"
  ],
  "plugins": [
    ["react-native-web", { "commonjs": true }],
    ["nativewind/babel", { "allowModuleTransform": ["moti"] }]
  ]
}
