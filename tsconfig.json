{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext",
      "DOM",
      "WebWorker"
    ],
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "exclude": [
    "dev-utils"
  ],
}