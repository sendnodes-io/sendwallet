{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^lint"],
      "outputs": ["dist/**", "types/**", ".next/**"]
    },
    "test": {
      "dependsOn": ["build", "lint"],
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "lint": {
      "dependsOn": ["^//#lint:prettier", "^//#lint:tsc", "^@my/ui#build"],
      "outputs": []
    },
    "//#lint:prettier": {
      "outputs": []
    },
    "//#lint:tsc": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": [],
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": [],
      "dependsOn": ["^//#clean:turbo"]
    },
    "//#clean:turbo": {
      "cache": false,
      "outputs": []
    }
  }
}
